<h1>MAIN</h1>

<div class="card-deck container-fluid">
    {{#each cards}}
    <div class="mycard col-3">
      <div class="mycard-body">
        <div class="volume-main-icon"><img src="../images/volume.png" alt="text-to-speech"></div>
        <h3 class="mycard-title"> {{phrase}} </h3>
        <div class="thumbsup-icon">
          <form action="/main/{{_id}}" method="POST">
            <button type="submit" class="btn-thumbsup-icon">
              <img src="../images/thumbs_up.png" alt="like-button">
            </button>
          </form>
      </div>
      </div>
    </div>
    {{/each}}
  </div>